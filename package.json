{
  "name": "tree-sitter-symon",
  "version": "0.0.1",
  "description": "High-level language for a symbolic monitor --- SyMon",
  "repository": "https://github.com/maswag/symon",
  "license": "GPLv3",
  "author": {
    "name": "Masaki Waga",
    "email": "masakiwaga@gmail.com",
    "url": "https://www.fos.kuis.kyoto-u.ac.jp/~mwaga/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "symon"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
